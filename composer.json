{
    "name": "eevgbut/spreadsheet",
    "description": "Yii2 extension for export to a spreadsheet, e.g. Excel, LibreOffice etc.",
    "keywords": ["yii2", "spreadsheet", "excel", "libreoffice", "export", "xls", "xlsx"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yii2tech/spreadsheet/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "https://github.com/yii2tech/spreadsheet/wiki",
        "source": "https://github.com/yii2tech/spreadsheet"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.13",
        "eevgbut/phpspreadsheet": "^1.30.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^6.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "git",
            "name": "eevgbut/phpspreadsheet",
            "url": "https://github.com/eevgbut/PhpSpreadsheet.git"
        }
    ],
    "autoload": {
        "psr-4": { "yii2tech\\spreadsheet\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
